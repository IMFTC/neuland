msgid "Never"
msgstr "Nie"

#. start_today <= last
#. Translators: Time in 24h format
#: ../src/neuland-contact.c:214
msgid "%H:%M"
msgstr "%H:%M"

#. start_yesterday <= last
#. Translators: This is "Yesterday" followed by a time string in 24h format
#: ../src/neuland-contact.c:220
msgid "Yesterday, %H:%M"
msgstr "Gestern, %H:%M"

#. 6 or less days ago (not 7 days ago, because we don't want
#. to show Monday 10:12 when today is Monday, too).
#. Translators: This is a abbreviated week day name
#. followed by a time string in 24h format
#: ../src/neuland-contact.c:228
msgid "%a, %H:%M"
msgstr "%a, %H:%M"

#. start_year <= last
#. Translators: This is the abbreviated month name and day
#. number followed by a time string in 24h format
#: ../src/neuland-contact.c:235
msgid "%b %d, %H:%M"
msgstr "%d. %b, %H:%M"

#. last < start_year
#. Translators: This is the abbreviated month name, day
#. number, year number followed by a time string in 24h
#. format
#: ../src/neuland-contact.c:243
msgid "%b %d %Y, %H:%M"
msgstr "%d.%m.%Y, %H:%M"

#. start_today <= last
#. Translators: Time in 12h format
#: ../src/neuland-contact.c:250
msgid "%l:%M %p"
msgstr "%l:%M %p"

#. start_yesterday <= last
#. Translators: This is "Yesterday" followed by a time string in 12h format
#: ../src/neuland-contact.c:254
msgid "Yesterday, %l:%M %p"
msgstr "Gestern, %l:%M %p"

#. 6 or less days ago (not 7 days ago, because we don't want
#. to show Monday 10:12 when today is Monday, too).
#. Translators: This is a abbreviated week day name
#. followed by a time string in 12h format
#: ../src/neuland-contact.c:262
msgid "%a, %l:%M %p"
msgstr "%a, %l:%M %p"

#. start_year <= last
#. Translators: This is the abbreviated month name, day
#. number followed by a time string in 12h format
#: ../src/neuland-contact.c:268
msgid "%b %d, %l:%M %p"
msgstr "%d. %b, %l:%M %p"

#. last < start_year
#. Translators: This is the abbreviated month name, day
#. number, year number followed by a time string in 12h
#. format.
#: ../src/neuland-contact.c:275
msgid "%b %d %Y, %l:%M %p"
msgstr "%d.%m.%Y, %l:%M %p"
